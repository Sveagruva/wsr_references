<Window x:Class="convertor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:convertor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <local:Conv x:Key="convertic"></local:Conv>
    </Window.Resources>
    <DataGrid x:Name="gridik">
        <DataGrid.Columns>
            <DataGridTextColumn Header="typebitch" Binding="{Binding Mode=OneWay, Converter={StaticResource convertic}}"></DataGridTextColumn>
        </DataGrid.Columns>
    </DataGrid>
</Window>
